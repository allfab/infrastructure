port: 5000
wipe_influxdb: false
debug: true
ssl:
  gateway: true
  certfile: ''  
  keyfile: ''
tempo:
  enable: true
home_assistant:
  discovery: true
  discovery_prefix: homeassistant
  enable: true
home_assistant_ws:
  enable: true
  ssl: false
  token: "{{ myelectricaldata.homeassistant.token }}"
  url: "{{ myelectricaldata.homeassistant.server }}"
influxdb:
  batching_options:
    batch_size: 1000
    exponential_base: 2
    flush_interval: 1000
    jitter_interval: 0
    max_retries: 5
    max_retry_delay: 125000
    max_retry_time: 180000
    retry_interval: 5000
  bucket: myelectricaldata
  enable: false
  hostname: influxdb
  method: synchronous
  org: myelectricaldata
  port: 8086
  token: myelectricaldata
log2file: true
mqtt:
  client_id: myelectricaldata
  enable: true
  hostname: "{{ mqtt.ip }}"
  username: "{{ mqtt.user }}"
  password: "{{ mqtt.password }}"
  port: 1883
  prefix: myelectricaldata
  qos: 0
  retain: true  
myelectricaldata:
  '{{ myelectricaldata.pdl.name }}':
    addresses: 'true'
    cache: 'true'
    consumption: 'true'
    consumption_detail: 'true'
    consumption_detail_max_date: '2023-01-01'
    consumption_max_date: '2023-01-01'
    consumption_max_power: 'true'
    consumption_price_base: '0.1369'
    consumption_price_hc: '0.1056'
    consumption_price_hp: '0.1369'
    enable: 'true'
    name: Maison
    offpeak_hours_0: 22H00-6H00
    offpeak_hours_1: 22H00-6H00
    offpeak_hours_2: 22H00-6H00
    offpeak_hours_3: 22H00-6H00
    offpeak_hours_4: 22H00-6H00
    offpeak_hours_5: 22H00-6H00
    offpeak_hours_6: 22H00-6H00
    plan: Tempo
    production: 'false'
    production_detail: 'false'
    production_detail_max_date: ''
    production_max_date: ''
    production_price: '0.0'
    refresh_addresse: 'false'
    refresh_contract: 'false'
    token: "{{ myelectricaldata.pdl.token }}"
