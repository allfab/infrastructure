http:
  routers:
    homeassistant:
      entryPoints:
        - websecure
      rule: 'Host(`domotik.allfabox.fr`)'
      service: homeassistant
    nodered:
      entryPoints:
        - websecure
      rule: 'Host(`nodered.allfabox.fr`)'
      service: nodered
      middlewares:
        - authelia
    zigbee2mqtt:
      entryPoints:
        - websecure
      rule: 'Host(`zigbee2mqtt.allfabox.fr`)'
      service: zigbee2mqtt
      middlewares:
        - authelia
    esphome:
      entryPoints:
        - websecure
      rule: 'Host(`esphome.allfabox.fr`)'
      service: esphome
      middlewares:
        - authelia
  
  services:
    homeassistant:
      loadBalancer:
        servers:
          - url: http://192.168.10.20:8123
    nodered:
      loadBalancer:
        servers:
          - url: http://192.168.10.20:1880
    zigbee2mqtt:
      loadBalancer:
        servers:
          - url: http://192.168.10.20:8080
    esphome:
      loadBalancer:
        servers:
          - url: http://192.168.10.20:6052